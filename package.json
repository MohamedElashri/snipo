    "htmx.org": "^2.0.8",
    "alpinejs": "^3.15.3",
    "@alpinejs/collapse": "^3.15.3",
    "ace-builds": "^1.43.5",
    "prismjs": "^1.30.0",
    "marked": "^17.0.1",
    "@picocss/pico": "^2.1.1"
  },
  "devDependencies": {
    "fs-extra": "^11.3.3"
  }
}
