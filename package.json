{
  "name": "snipo-frontend-vendor",
  "version": "1.0.0",
  "description": "Frontend vendor dependencies for Snipo - managed locally for privacy",
  "private": true,
  "scripts": {
    "vendor:sync": "node scripts/sync-vendor.js",
    "vendor:check": "npm outdated",
